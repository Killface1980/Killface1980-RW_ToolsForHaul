  <!-- ============================ Vehicle ========================== -->

  <ThingDef  Name="VehicleBase" Abstract="True">
    <category>Building</category>
    <selectable>true</selectable>
    <tickerType>Normal</tickerType>
    <passability>PassThroughOnly</passability>
    <altitudeLayer>Building</altitudeLayer>
    <useHitPoints>True</useHitPoints>
    <hasTooltip>true</hasTooltip>
    <soundImpactDefault>BulletImpactMetal</soundImpactDefault>
    <blockLight>true</blockLight>
    <building>
      <ai_combatDangerous>true</ai_combatDangerous>
      <isEdifice>true</isEdifice>
      <repairable>true</repairable>
      <isSittable>true</isSittable>
      <expandHomeArea>false</expandHomeArea>
      <allowWireConnection>false</allowWireConnection>
      <bed_humanlike>false</bed_humanlike>
      <isInert>true</isInert>
      <deconstructible>false</deconstructible>
      <isJoySource>true</isJoySource>
    </building>
    <damageMultipliers>
      <li>
        <damageDef>Bomb</damageDef>
        <multiplier>2.5</multiplier>
      </li>
      <li>
        <damageDef>Flame</damageDef>
        <multiplier>2.5</multiplier>
      </li>
      <li>
        <damageDef>Bullet</damageDef>
        <multiplier>1.0</multiplier>
      </li>
      <li>
        <damageDef>Crush</damageDef>
        <multiplier>0.5</multiplier>
      </li>
      <li>
        <damageDef>Arrow</damageDef>
        <multiplier>0.3</multiplier>
      </li>
      <li>
        <damageDef>EMP</damageDef>
        <multiplier>0.8</multiplier>
      </li>
    </damageMultipliers>
    <fillPercent>0.4</fillPercent>
    <statBases>
      <Flammability>0.05</Flammability>
      <SellPriceFactor>0.3</SellPriceFactor>
    </statBases>
    <alwaysHaulable>false</alwaysHaulable>
    <designateHaulable>false</designateHaulable>
    <comps>
      <li Class="CompProperties_Forbiddable"/>
      <li>
        <compClass>ToolsForHaul.Components.CompMountable</compClass>
      </li>
      <li>
        <compClass>CompQuality</compClass>
      </li>
      <li>
        <compClass>CompColorable</compClass>
      </li>
    </comps>
  </ThingDef>

  <ThingDef ParentName="VehicleBase">
    <defName>VehicleCart</defName>
    <label>cart</label>
    <repairEffect>ConstructWood</repairEffect>
    <hasInteractionCell>True</hasInteractionCell>
    <interactionCellOffset>(0,0,1)</interactionCellOffset>
    <altitudeLayer>Building</altitudeLayer>
    <description>A cart is an old-fashioned wooden vehicle that is used for transporting goods or people.</description>
    <thingClass>ToolsForHaul.Vehicle_Cart</thingClass>
    <thingCategories>
      <li>Vehicles</li>
    </thingCategories>
    <graphicData>
      <texPath>Things/Pawn/VehicleCart/Cart</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <drawSize>(3,3)</drawSize>
      <damageData>
        <!-- no damage marks because they don't move with the vehicle <rect>(0,0.12,1,0.76)</rect>-->
        <enabled>false</enabled>
      </damageData>
    </graphicData>
    <statBases>
      <MaxHitPoints>150</MaxHitPoints>
      <VehicleSpeed>0.75</VehicleSpeed>
      <VehicleMaxItem>4</VehicleMaxItem>
    </statBases>
    <size>(1,1)</size>
    <fillPercent>0.99</fillPercent>
    <inspectorTabs>
      <li>ToolsForHaul.Itabs.Itab_Pawn_Vehicle_Storage</li>
      <li>ToolsForHaul.Itabs.ITab_Pawn_VehicleGear</li>
    </inspectorTabs>
    <CostList>
      <Steel>40</Steel>
      <WoodLog>120</WoodLog>
    </CostList>
    <leaveResourcesWhenKilled>true</leaveResourcesWhenKilled>
    <killedLeavings>
      <Steel>40</Steel>
    </killedLeavings>
    <recipeMaker>
      <defaultIngredientFilter>
        <categories>
          <li>Root</li>
        </categories>
        <exceptedThingDefs>
          <li>Silver</li>
          <li>Gold</li>
        </exceptedThingDefs>
      </defaultIngredientFilter>
      <workSpeedStat>SmithingSpeed</workSpeedStat>
      <workSkill>Crafting</workSkill>
      <effectWorking>Smith</effectWorking>
      <soundWorking>Recipe_Smith</soundWorking>
      <recipeUsers>
        <li>FueledSmithy</li>
        <li>ElectricSmithy</li>
        <li>TableMachining</li>
      </recipeUsers>
      <workAmount>2400</workAmount>
      <unfinishedThingDef>UnfinishedCart</unfinishedThingDef>
      <skillRequirements>
        <!-- <li>
		  <skill>Crafting</skill>
		  <minLevel>8</minLevel>
		</li> -->
      </skillRequirements>
    </recipeMaker>
    <comps>
      <li Class="ToolsForHaul.Components.CompProperties_Axles">
        <axles>
          <li>(-20,-40)</li>
        </axles>
      </li>
      <li Class="ToolsForHaul.Components.CompProperties_Vehicles">
        <showsStorage>true</showsStorage>
        <animalsCanDrive>true</animalsCanDrive>
        <fuelCatchesFireHitPointsPercent>0.0</fuelCatchesFireHitPointsPercent>
        <motorizedWithoutFuel>false</motorizedWithoutFuel>
      <!--
      <soundAmbient>VehicleATV_Ambience</soundAmbient>
        -->  
          <specialshadowData>
          <volume>(0.65, 0.6, 0.5)</volume>
          <offset>(0,0,-0.3)</offset>
        </specialshadowData>
      </li>
    </comps>
  </ThingDef>

